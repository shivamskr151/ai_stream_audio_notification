datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id                      String    @id @default(uuid())
  audio_url               String
  image_url               String
  timestamp               DateTime
  event_type              String
  created_at              DateTime  @default(now())
  updated_at              DateTime  @updatedAt  
}

